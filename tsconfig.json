{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es5",
        "lib": ["es6", "dom", "DOM.Iterable", "ESNext"],
        "sourceMap": true,
        "allowJs": false,
        "jsx": "react",
        "moduleResolution": "node",
        "rootDir": "src",
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "noImplicitAny": true,
        "strictNullChecks": true,
        "esModuleInterop": true,
        "removeComments": true,
        "baseUrl": "./",
        "paths": {
            "Assets/*": ["src/assets/*"],
            "Ts/*": ["src/ts/*"],
            "Components/*": ["src/ts/components/*"],
            "App/*": ["src/ts/app/*"],
            "Redux/*": ["src/ts/redux/*"],
            "Actions/*": ["src/ts/redux/actions/*"],
            "Selectors/*": ["src/ts/redux/selectors/*"],
            "Services/*": ["src/ts/services/*"],
            "Utils/*": ["src/ts/utils/*"]
        }
    },
    "exclude": [
        "node_modules",
        "build",
        "scripts",
        "acceptance-tests",
        "webpack",
        "jest",
        "src/setupTests.ts"
    ],
    "types": ["typePatches"]
}
